<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:userControls="clr-namespace:Avalonia.Demo.UserControls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Avalonia.Demo.UserControls.AdminUserControl">
    <TabControl>
        <TabItem Header="Услуги">
            <userControls:ServiceUserControl Grid.Row="1"/>
        </TabItem>
        <TabItem Header="Добавление услуги">
            <userControls:InsertServiceUserControl/>
        </TabItem>
        <TabItem Header="Ближайшие записи">
            <userControls:UpcomingEntriesUserControl/>
        </TabItem>
    </TabControl>
</UserControl>
